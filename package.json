{
  "name": "Keira3",
  "version": "3.3.0",
  "description": "Database Editor for AzerothCore",
  "homepage": "https://github.com/AzerothCore/Keira3",
  "author": {
    "name": "Francesco Borzi'",
    "email": "borzifrancesco@gmail.com",
    "url": "https://github.com/FrancescoBorzi"
  },
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "format": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --no-editorconfig --write .",
    "format-staged": "pretty-quick --staged",
    "test": "ng test --watch=false",
    "test-ci": "ng test --watch=false --browsers ChromeHeadless --code-coverage && (cat ./coverage/lcov.info | coveralls || true)",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "lint": "ng lint",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/common": "12.2.13",
    "@angular/compiler": "12.2.13",
    "@angular/core": "12.2.13",
    "@angular/forms": "12.2.13",
    "@angular/language-service": "12.2.13",
    "@angular/platform-browser": "12.2.13",
    "@angular/platform-browser-dynamic": "12.2.13",
    "@angular/router": "12.2.13",
    "@types/electron-settings": "4.0.0",
    "acorn": "8.5.0",
    "electron-settings": "4.0.2",
    "karma-coverage": "2.0.3",
    "mysql": "2.18.1",
    "ngx-toastr": "14.2.0",
    "ngx-ui-switch": "12.0.1",
    "rxjs": "6.6.7",
    "serialize-javascript": "6.0.0",
    "sqlite3": "4.2.0",
    "squel": "5.13.0",
    "tslib": "2.3.1",
    "webpack-sources": "3.2.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.3",
    "@angular-devkit/build-angular": "12.2.13",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/animations": "12.2.13",
    "@angular/cli": "12.2.13",
    "@angular/compiler-cli": "12.2.13",
    "@fortawesome/fontawesome-free": "5.15.4",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@playwright/test": "1.16.3",
    "@swimlane/ngx-datatable": "20.0.0",
    "@types/jasmine": "3.10.2",
    "@types/jasminewd2": "2.0.10",
    "@types/mysql": "2.15.19",
    "@types/node": "16.11.7",
    "@types/sqlite3": "3.1.7",
    "@types/sqlstring": "2.3.0",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "bootstrap": "5.1.3",
    "conventional-changelog-cli": "2.1.1",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "electron": "15.3.1",
    "electron-builder": "22.13.1",
    "electron-reload": "1.5.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.4.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "highlight.js": "10.7.3",
    "husky": "7.0.4",
    "jasmine-core": "3.10.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.8",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.0.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lint-staged": "12.0.2",
    "nan": "2.14.2",
    "ngx-bootstrap": "7.1.0",
    "ngx-clipboard": "14.0.0",
    "ngx-highlightjs": "5.0.0",
    "ngx-perfect-scrollbar": "10.1.1",
    "ngx-typesafe-forms": "1.5.2",
    "node-polyfill-webpack-plugin": "1.1.4",
    "npm-run-all": "4.1.5",
    "playwright": "1.16.3",
    "sqlstring": "2.3.2",
    "ts-node": "10.4.0",
    "typescript": "4.3.5",
    "wait-on": "6.0.0",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts}": [
      "npm run format-staged",
      "git add"
    ]
  },
  "browserslist": [
    "chrome 91"
  ]
}
